<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<!DOCTYPE funcs SYSTEM "../../../clonk.dtd">
<?xml-stylesheet type="text/xsl" href="../../../clonk.xsl"?>
<funcs>
	<func>
		<title>C4Id</title>
		<category>Script</category><subcat>Strings</subcat>
		<version>4.6.5.0 CP</version>
		<syntax>
			<rtype>id</rtype>
			<params>
				<param><type>string</type><name>szID</name><desc>Der String, der in eine id umgewandelt werden soll.</desc></param>
			</params>
		</syntax>
		<desc>Wandelt einen String in eine ID um.</desc>
		<examples>
			<example>
				<code>C4Id("ROCK")</code>
				<text>Gibt die ID 'ROCK' zurück.</text>
			</example>
		</examples>
	</func>
	<author>jwk</author><date>April 2002</date>
</funcs>
