<funcs>
	<func>
		<title>SetAlive</title>
		<category>Objekte</category><subcat>Lebewesen</subcat>
		<version>4.6.5.0 CP</version>
		<syntax>
			<rtype>int</rtype>
			<params>
				<param><type>bool</type><name>fAlive</name><desc>0 = Tot; ansonsten lebendig</desc></param>
				<param><type>object</type><name>pObj</name><desc>Objekt, dessen Zustand gesetzt werden soll. 0 bei lokalem Aufruf.</desc><optional /></param>
			</params>
		</syntax>
		<desc>Legt fest, ob ein Objekt lebt.</desc>
		<related>
			<funclink>Kill</funclink>
			<funclink>GetAlive</funclink>
		</related>
	</func>
